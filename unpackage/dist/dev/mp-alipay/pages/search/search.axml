<view class="data-v-4cedc0c6"><music-head vue-id="50c0d8d6-1" title="搜索" isShow="{{true}}" iconblack="{{true}}" class="data-v-4cedc0c6" onVueInit="__l"></music-head><view class="u-m-30 data-v-4cedc0c6"><u-search vue-id="50c0d8d6-2" clearabled="{{true}}" placeholder="搜索的歌曲/歌手" height="70" value="{{keyword}}" data-event-opts="{{[['^search',[['handleToSearch',['$0'],['keyword']]]],['^custom',[['handleToSearch',['$0'],['keyword']]]],['^change',[['ToChange']]],['^clear',[['ClearClick']]],['^input',[['__set_model',['','keyword','$event',[]]]]]]}}" onSearch="__e" onCustom="__e" onChange="__e" onClear="__e" onInput="__e" class="data-v-4cedc0c6" onVueInit="__l"></u-search></view><view class="container data-v-4cedc0c6"><scroll-view scroll-y="true" class="data-v-4cedc0c6"><block a:if="{{searchType==1}}"><block class="data-v-4cedc0c6"><view class="search-history data-v-4cedc0c6"><view class="search-history-head data-v-4cedc0c6"><text class="data-v-4cedc0c6">历史记录</text><text data-event-opts="{{[['tap',[['ClearSearch',['$event']]]]]}}" class="iconfont icon-shanchu data-v-4cedc0c6" onTap="__e"></text></view><view class="search-history-list data-v-4cedc0c6"><view data-event-opts="{{[['tap',[['handleToWord',['$0'],[[['searchHistory','',index]]]]]]]}}" onTap="__e" class="data-v-4cedc0c6" a:for="{{searchHistory}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></view></view><view class="search-hot data-v-4cedc0c6"><view class="search-hot-head data-v-4cedc0c6">热搜榜</view><view data-event-opts="{{[['tap',[['handleToWord',['$0'],[[['SearchHot','',index,'searchWord']]]]]]]}}" class="search-hot-item data-v-4cedc0c6" onTap="__e" a:for="{{SearchHot}}" a:for-item="item" a:for-index="index" a:key="index"><view class="search-hot-top data-v-4cedc0c6">{{index+1}}</view><view class="search-hot-word data-v-4cedc0c6"><view class="data-v-4cedc0c6">{{''+item.searchWord}}<image src="{{item.iconUrl}}" mode="aspectFit" class="data-v-4cedc0c6"></image></view><view class="data-v-4cedc0c6">{{item.content}}</view></view><text class="search-hot-count data-v-4cedc0c6">{{item.score}}</text></view></view></block></block><block a:else><block a:if="{{searchType==2}}"><block class="data-v-4cedc0c6"><view class="search-result data-v-4cedc0c6"><view data-event-opts="{{[['tap',[['ToDetail',['$0'],[[['searchList','',index,'id']]]]]]]}}" class="search-result-item data-v-4cedc0c6" onTap="__e" a:for="{{searchList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="search-result-word data-v-4cedc0c6"><view class="data-v-4cedc0c6">{{item.name}}</view><view class="data-v-4cedc0c6"><block a:if="{{item.mark>60&&item.mark<70}}"><image src="/static/dujia.png" class="data-v-4cedc0c6"></image></block><view class="data-v-4cedc0c6">{{item.artists[0].name+" - "+item.album.name}}</view></view></view><text class="iconfont icon-bofang data-v-4cedc0c6"></text></view></view></block></block></block><block a:if="{{searchType==3}}"><block class="data-v-4cedc0c6"><view class="search-suggest data-v-4cedc0c6"><view class="search-suggest-head data-v-4cedc0c6">{{"搜索“"+keyword+"”"}}</view><view data-event-opts="{{[['tap',[['handleToWord',['$0'],[[['searchHint','',index,'keyword']]]]]]]}}" class="search-suggest-item data-v-4cedc0c6" onTap="__e" a:for="{{searchHint}}" a:for-item="item" a:for-index="index" a:key="index"><text class="iconfont icon-sousuo_2 data-v-4cedc0c6"></text>{{''+item.keyword+''}}</view></view></block></block></scroll-view></view></view>